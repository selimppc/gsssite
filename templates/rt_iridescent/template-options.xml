		</fields>

		<!-- Top -->
		<fields name="top" type="selectedset" label="TOP_STYLE" description="TOP_STYLE_DESC">
			<field name="textcolor" type="colorchooser" default="#666666" label="TEXT_COLOR" description="TEXT_COLOR_DESC" setinsession="true" setbysession="true" setincookie="true" setbycookie="true" />
			<field name="background" type="colorchooser" default="#FFFFFF" label="BACKGROUND_COLOR" description="BACKGROUND_COLOR_DESC" setinsession="true" setbysession="true" setincookie="true" setbycookie="true" />
		</fields>

		<!-- Showcase -->
		<fields name="showcase" type="selectedset" label="SHOWCASE_STYLE" description="SHOWCASE_STYLE_DESC">
			<field name="textcolor" type="colorchooser" default="#7E7E7E" label="TEXT_COLOR" description="TEXT_COLOR_DESC" setinsession="true" setbysession="true" setincookie="true" setbycookie="true" />
			<field name="background" type="colorchooser" default="#000000" label="BACKGROUND_COLOR" description="BACKGROUND_COLOR_DESC" setinsession="true" setbysession="true" setincookie="true" setbycookie="true" />
		</fields>

		<!-- Feature -->
		<fields name="feature" type="selectedset" label="FEATURE_STYLE" description="FEATURE_STYLE_DESC">
			<field name="textcolor" type="colorchooser" default="#FFFFFF" label="TEXT_COLOR" description="TEXT_COLOR_DESC" setinsession="true" setbysession="true" setincookie="true" setbycookie="true" />
			<field name="background" type="colorchooser" default="#DF172E" label="BACKGROUND_COLOR" description="BACKGROUND_COLOR_DESC" setinsession="true" setbysession="true" setincookie="true" setbycookie="true" />
		</fields>

		<!-- Utility -->
		<fields name="utility" type="selectedset" label="UTILITY_STYLE" description="UTILITY_STYLE_DESC">
			<field name="textcolor" type="colorchooser" default="#FFFFFF" label="TEXT_COLOR" description="TEXT_COLOR_DESC" setinsession="true" setbysession="true" setincookie="true" setbycookie="true" />
			<field name="background" type="colorchooser" default="#222222" label="BACKGROUND_COLOR" description="BACKGROUND_COLOR_DESC" setinsession="true" setbysession="true" setincookie="true" setbycookie="true" />
		</fields>

		<!-- Extension -->
		<fields name="extension" type="selectedset" label="EXTENSION_STYLE" description="EXTENSION_STYLE_DESC">
			<field name="textcolor" type="colorchooser" default="#FFFFFF" label="TEXT_COLOR" description="TEXT_COLOR_DESC" setinsession="true" setbysession="true" setincookie="true" setbycookie="true" />
			<field name="background" type="colorchooser" default="#DF172E" label="BACKGROUND_COLOR" description="BACKGROUND_COLOR_DESC" setinsession="true" setbysession="true" setincookie="true" setbycookie="true" />
		</fields>

		<!-- Bottom -->
		<fields name="bottom" type="selectedset" label="BOTTOM_STYLE" description="BOTTOM_STYLE_DESC">
			<field name="textcolor" type="colorchooser" default="#7E7E7E" label="TEXT_COLOR" description="TEXT_COLOR_DESC" setinsession="true" setbysession="true" setincookie="true" setbycookie="true" />
			<field name="background" type="colorchooser" default="#000000" label="BACKGROUND_COLOR" description="BACKGROUND_COLOR_DESC" setinsession="true" setbysession="true" setincookie="true" setbycookie="true" />
		</fields>

		<!-- Footer -->
		<fields name="footer" type="selectedset" label="FOOTER_STYLE" description="FOOTER_STYLE_DESC">
