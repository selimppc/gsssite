		</fields>

		<!-- Copyright -->
		<fields name="copyright" type="selectedset" label="COPYRIGHT_STYLE" description="COPYRIGHT_STYLE_DESC">
			<field name="textcolor" type="colorchooser" default="#7E7E7E" label="TEXT_COLOR" description="TEXT_COLOR_DESC" setinsession="true" setbysession="true" setincookie="true" setbycookie="true" />
			<field name="background" type="colorchooser" default="#000000" label="BACKGROUND_COLOR" description="BACKGROUND_COLOR_DESC" setinsession="true" setbysession="true" setincookie="true" setbycookie="true" />
		</fields>

		<!-- Font -->
		<fields name="font" type="chain" label="FONT_SETTINGS" description="FONT_SETTINGS_DESC">
			<field name="family" type="fonts" default="s:iridescent" primary="iridescent" label="FONT_FAMILY" isbodyclass="true" setbyurl="true" setinsession="true" setbysession="true" setincookie="true" setbycookie="true">
