			</fields>
		</fields>

		<!-- Demo Style -->
		<fields name="demostyle" type="selectedset" label="DEMOSTYLE" description="DEMOSTYLE_DESC">
			<field name="type" type="selectbox" selector="true" default="preset1" label="TYPE" isbodyclass="true" setbyurl="true" setinsession="true" setbysession="true" setincookie="true" setbycookie="true"/>
